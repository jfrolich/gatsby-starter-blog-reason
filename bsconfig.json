{
  "name": "gatsby-starter-reasonml",
  "refmt": 3,
  "reason": {
    "react-jsx": 3
  },
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js",
  "sources": [
    {
      "dir": "./src/",
      "subdirs": true
    },
    {
      "dir": "./config/",
      "subdirs": true
    }
  ],
  "ppx-flags": [
    ["@reasonml-community/graphql-ppx/ppx", "-template-tag=graphql"]
  ],
  "bs-dependencies": ["reason-react", "reason-gatsby"],
  "warnings": {
    "number": "-30"
  }
}
