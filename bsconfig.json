{
  "name": "gatsby-starter-reasonml",
  "refmt": 3,
  "reason": {
    "react-jsx": 3
  },
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js",
  "sources": [
    {
      "dir": "./src/",
      "subdirs": true
    },
    {
      "dir": "./config/",
      "subdirs": true
    }
  ],
  "ppx-flags": [
    [
      "@baransu/graphql_ppx_re/ppx",
      "-records",
      "-template-tag-import=graphql",
      "-template-tag-location=gatsby"
    ]
  ],
  "bs-dependencies": ["reason-react", "@jfrolich/reason-gatsby"],
  "warnings": {
    "number": "-30"
  }
}
